<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
	
<struts>
	<package name="TMT" extends="struts-default, tiles-default">
		<action name="Main" class="main.Main">
			<result name="success" type="tiles">Main</result>
		</action>
		
		<action name="AirList" class="item.air.AirList">
			<result name="success" type="tiles">AirList</result>
		</action>
		
		<action name="TourList" class="item.tour.TourList">
			<result name="success" type="tiles">TourList</result>
		</action> 
		
		<action name="HotelList" class="item.hotel.HotelList">
			<result name="success" type="tiles">HotelList</result>
		</action>
		
		<action name="Login" class="member.LoginMember" method="Login">
			<result name="success" type="tiles">Login</result>
		</action>		
		

		<action name="DeleteMemberCheck" class="member.DeleteMember">
			<result name="success" type="tiles">DeleteMemberCheck</result>
		</action>
		
		<action name="LoginForm" class="member.LoginMember">
			<result name="success" type="tiles">LoginForm</result>
			<result name="error">/member/loginError.jsp</result>

		</action>
				
		<action name="Join" class="member.JoinMember" method="Join">
			<result name="success" type="tiles">Join</result>
		</action>
		
	    <action name="JoinForm" class="member.JoinMember">
			<result name="success" type="tiles">JoinForm</result>
		</action>
		
		<action name="ConfirmId" class="member.JoinMemberIdChk">
			<result name="success" type="tiles">ConfirmId</result>
		</action>
		

		<action name="ModifymemberForm" class="member.ModifyMember">
			<result name="success" type="tiles">ModifyMemberForm</result>
		</action>

	</package>
	<include file="struts/admin.xml" />
	<include file="struts/notice.xml" />

	<!-- 항공 -->
	<include file="/struts/air.xml" />

</struts>